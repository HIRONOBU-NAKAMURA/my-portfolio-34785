<% provide(:title, '全てのユーザ一') %>
<h1>全てのユーザー</h1>

<%= will_paginate @users , previous_label: '&lt 前へ', next_label: '次へ &gt'%>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user %>
      <%= link_to user.name, my_account_path(user) %>
    </li>
  <% end %>
</ul>

<%= will_paginate @users , previous_label: '&lt 前へ', next_label: '次へ &gt'%>
